      </ImageView>
      <ImageView fx:id="hintMsg" fitHeight="199.0" fitWidth="218.0" layoutX="946.0" layoutY="537.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../images/hintMsg.png" />
